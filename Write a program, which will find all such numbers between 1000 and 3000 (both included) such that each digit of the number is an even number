def all_even_digits(num):
    # Check if all digits in the number are even
    return all(int(digit) % 2 == 0 for digit in str(num))

# Find and print numbers with all even digits between 1000 and 3000
result = [num for num in range(1000, 3001) if all_even_digits(num)]

print("Numbers between 1000 and 3000 with all even digits:")
print(result)
